{
	"name": "flarum/serbiancyrlanguage",
	"type": "flarum-extension",
	"description": "Serbian cyrillic language pack for Flarum.",
	"keywords": [
		"flarum",
		"serbian",
		"cyrillic",
		"language"
	],
	"license": "MIT",
	"homepage": "https://flarum.org",
	"authors": [
		{
			"name": "Nikola Radovanovic",
			"email": "cobisimo@gmail.com"
		}
	],
	"support": {
		"issues": "https://github.com/cobisimo/flarum-lang-serbian-cyr/issues",
		"source": "https://github.com/cobisimo/flarum-lang-serbian-cyr"
	},
	"require": {
		"flarum/core": "^1.2"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.x-dev"
		},
		"flarum-extension": {
			"title": "Serbian (cyr)",
			"icon": {
				"image": "icon.svg",
				"backgroundColor": "#ffffff",
				"backgroundSize": "cover",
				"backgroundPosition": "center"
			}
		},
		"flarum-locale": {
			"code": "sr",
			"title": "Српски"
		}
	}
}
